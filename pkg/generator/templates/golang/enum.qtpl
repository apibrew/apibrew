{% import "github.com/apibrew/apibrew/pkg/model" %}

{%- func GenerateEnumCode(resource *model.Resource, enumProperty *model.ResourceProperty) -%}
{%- code typeName := PropPureGoType(resource, enumProperty) -%}

type {%s typeName %} string

const (
{%- for _, value := range enumProperty.EnumValues -%}
    {%s typeName %}_{%= GoName(value) %} {%s typeName %} = "{%s value %}"
{%- endfor -%}
)
{% endfunc %}