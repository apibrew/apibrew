{% import "github.com/apibrew/apibrew/pkg/model" %}

{%- func GenerateInlineStructCode(resource*model.Resource, properties map[string]*model.ResourceProperty) -%}
struct {
    {%- for propName, property := range properties -%}
	    {%= GoName(propName) %} {%= PropertyType(resource, property) %}
	{%- endfor -%}
}{%- endfunc -%}