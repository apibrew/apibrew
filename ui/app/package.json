{
  "name": "@apibrew/app",
  "private": false,
  "version": "1.0.24",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-ui-model": "apbr apply -f 'schema/*.yml'; apbr generate --path=src/model/ui --namespace=ui --platform=typescript; apbr generate --path=src/model/system --namespace=system --platform=typescript; apbr generate --path=src/model/logic --namespace=logic --platform=typescript",
    "build-ui-model:watch": "nodemon --watch schema -e \"yml\" --exec \"yarn build-ui-model\"",
    "build-ui-config": "apbr apply -f 'config/*.yml'",
    "build-ui-config:watch": "nodemon --watch config -e \"yml\" --exec \"yarn build-ui-config\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@types/pino": "^7.0.5",
    "pino": "^8.14.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
