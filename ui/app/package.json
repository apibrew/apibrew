{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/standalone": "^7.21.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@mui/x-data-grid": "^6.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "axios": "^1.3.6",
    "babel-standalone": "^6.26.0",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "simplebar-react": "^3.2.4",
    "source-map-explorer": "^2.5.3",
    "styled-components": "^6.0.0-rc.1",
    "swagger-ui-react": "^4.18.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build-ui-model": "apbr apply -f ui-model.yml; apbr generate --path=src/model --namespace=ui --platform=typescript",
    "build-ui-model:watch": "nodemon --watch ui-model.yml --exec \"yarn build-ui-model\"",
    "build-ui-config": "apbr apply -f ui-config.yml",
    "build-ui-config:watch": "nodemon --watch ui-config.yml --exec \"yarn build-ui-config\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/babel__standalone": "^7.1.4",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "typescript": "^5.0.4"
  }
}
